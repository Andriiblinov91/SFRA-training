<isloop items="${pdict.products}" var="product" status="loopstatus">
    <div class="col-6 col-sm-3 mt-4">
        <isobject object="${product}" view="searchhit">
           <isinclude url="${URLUtils.url('Tile-Show', 'pid', product.productID, 'swatches', true, 'ratings', true)}" />
        </isobject>
    </div>
</isloop>





